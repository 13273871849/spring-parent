# Auto Configure
org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
  com.sgrain.boot.cloud.httpclient.HttpClientBalanceAutoConfiguration,\
  com.sgrain.boot.cloud.retry.SmallGrainRibbonLoadBalancedRetryFactory

# Bootstrap Configuration
org.springframework.cloud.bootstrap.BootstrapConfiguration=\
  com.sgrain.boot.cloud.config.SmallGrainConsulConfigBootstrapConfiguration

#Initializers
org.springframework.context.ApplicationContextInitializer=\
  com.sgrain.boot.cloud.initializer.SmallGrainPropertySourceBootstrapConfiguration